<?xml version='1.0' encoding='UTF-8'?>
<configuration monitorInterval="30">
    <appenders>
        <Console name='Console' target='SYSTEM_ERR'>
            <PatternLayout pattern='%d{HH:mm:ss.SSS} %p [%t] %C{1}.%M: %m%n'/>
        </Console>
    </appenders>
    <loggers>
        <root level='DEBUG'>
            <appender-ref ref='Console'/>
        </root>
    </loggers>
</configuration>